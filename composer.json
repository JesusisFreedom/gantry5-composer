{
  "name": "gantry5-composer",
  "version": "5.1.0",
  "type": "wordpress-plugin",
  "description": "Gantry Framework Library",
  "license": "MIT",
  "minimum-stability": "dev",
  "config": {
    "vendor-dir": "src/vendor"
  },
  "require": {
    "php": ">=5.4.0",
    "symfony/yaml": "~2.7",
    "twig/twig": "~1.18",
    "pimple/pimple": "~3.0",
    "filp/whoops": "~1.2",
    "rockettheme/toolbox": "dev-develop",
    "leafo/scssphp": "~0.1",
    "leafo/lessphp": "~0.5",
    "erusev/parsedown-extra": "~0.7"
  },
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/rockettheme/toolbox"
    }
  ],
  "require-dev": {
    "phpunit/phpunit": "3.7.*"
  },
  "autoload": {
    "psr-4": {
      "Gantry\\": "src/classes/Gantry"
    }
  }
}
